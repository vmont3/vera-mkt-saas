# Application
PORT=3000
NODE_ENV=production
CORS_ORIGINS=https://app.quantumcert.com,https://admin.quantumcert.com

# Database
# Use sm:// notation for Google Secret Manager if implemented, otherwise standard connection string
DATABASE_URL=postgresql://user:password@localhost:5432/quantum_cert

# Security (JWT - RS256)
# Base64 encoded keys
JWT_PRIVATE_KEY=sm://quantum-cert/jwt-private-key
JWT_PUBLIC_KEY=sm://quantum-cert/jwt-public-key

# Blockchain (Algorand)
ALGORAND_ALGOD_TOKEN=sm://quantum-cert/algorand-token
ALGORAND_ALGOD_SERVER=https://mainnet-api.algonode.cloud
ALGORAND_PORT=443
ALGORAND_MNEMONIC=sm://quantum-cert/algorand-[REDACTED]

# KYC (Gov.br)
GOVBR_CLIENT_ID=sm://quantum-cert/govbr-client-id
GOVBR_CLIENT_SECRET=sm://quantum-cert/govbr-client-secret

# Google Cloud
GOOGLE_PROJECT_ID=quantum-cert-prod
